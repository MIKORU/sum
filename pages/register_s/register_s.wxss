.header {
  display: flex;
  justify-content: space-around;
  align-items: center;
  height: 180px;
  font-size: 28px;
}

.header_image {
  width: 101px;
  height: 93px;
  margin-top: 10px;
}

.body {
  display: flex;
  align-items: center;
  justify-content: space-around;
  height: 45px;
  font-size: 16px;
  color: #101010;
}

.image_1 {
  height: 25px;
  width: 30px;
  margin: 0px 15px;
}

.input_1 {
   display: flex;
    position: relative;  
   justify-content: space-between; 
   padding-left: 6px; 
  border-radius: 10px;
  align-items: center;
  margin-right: 10px; 
border: 1px solid #ddd;
  height: 30px;
  width: 100%;
}

.register {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 81px;
  height: 27px;
  border-radius: 70px;
  border: 1px solid red;
}

.cancle {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 81px;
  height: 27px;
  border-radius: 70px;
  border: 1px solid red;
}

.footer {
  display: flex;
  height: 70px;
  justify-content: space-around;
  align-items: center;
}
